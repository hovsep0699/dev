    "@babel/preset-typescript": "^7.8.3",
    "babel-jest": "^25.1.0",
    "jest": "^25.1.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "cadesplugin": "readonly"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.(spec|test|stories).{ts,tsx,js,jsx}",
      "!src/**/react-app-env.d.ts"
