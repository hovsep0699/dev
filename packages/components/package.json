{
  "name": "@distate/components",
  "version": "1.5.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "yarn build --watch",
    "build": "node scripts/copyIcons.js && tsc --build --preserveWatchOutput",
    "test": "jest",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "clean": "tsc --build --clean && rimraf dist",
    "storybook": "start-storybook --ci -s ./public -p 6006",
    "build:storybook": "build-storybook",
    "svgr": "rimraf src/icons/components && npx @svgr/cli --silent --ext tsx --config-file svgrconfig.js -d src/icons/components src/icons"
  },
  "dependencies": {
    "@distate/core": "^1.5.0",
    "add": "^2.0.6",
    "date-fns": "^2.22.1",
    "fast-deep-equal": "^3.1.3",
    "grommet": "^2.11.0",
    "moment": "^2.29.1",
    "polished": "^3.4.4",
    "react-fast-compare": "^3.2.0",
    "react-select": "^3.1.0",
    "styled-components": "^5.0.1",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-a11y": "^5.3.13",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-backgrounds": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addon-storysource": "^5.3.13",
    "@storybook/addons": "^5.3.12",
    "@storybook/react": "^5.3.12",
    "@svgr/webpack": "^5.1.0",
    "@types/json-schema": "^7.0.6",
    "@types/moment": "^2.13.0",
    "@types/react-select": "^3.0.11",
    "@types/styled-components": "^4.4.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "jest": "^25.1.0",
    "react-test-renderer": "^16.12.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.(spec|test|stories).{ts,tsx,js,jsx}",
      "!src/**/index.{ts,js}",
      "!src/**/react-app-env.d.ts"
    ]
  }
}
